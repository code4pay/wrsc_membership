@@ -0,0 +1,24 @@
+<?php
+
+namespace App\Models;
+
+use Illuminate\Database\Eloquent\Model;
+use Illuminate\Notifications\Notifiable;
+use Spatie\Permission\Traits\HasRoles;
+use Backpack\CRUD\app\Models\Traits\CrudTrait;
+class UserToPrimary extends Model
+{
+    protected $table = 'user_to_primary';
+    use CrudTrait;
+    use Notifiable;
+    use HasRoles;
+    public function primary() {
+    
+        return $this->belongsTo('\App\Models\BackpackUser','primary_user_id');
+    }
+
+    public function siblings() {
+        return $this->belongsTo('\App\Models\BackpackUser','sibling_user_id');
+    }
+
+}
